{
  "presets": [
    "@babel/preset-flow",
    [
      "@babel/preset-env",
      {
        "loose": true,
        "modules": "amd",
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7"]
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "add-module-exports",
    "transform-class-properties",
    "syntax-dynamic-import",
    "transform-es2015-modules-commonjs"
  ]
}
